<mvc:View controllerName="login.login.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <Page id="_IDGenPage" showHeader="true">
        <VBox id="_IDGenVBox" height="100%">
            <FlexBox id="_IDGenFlexBox" alignItems="Center" justifyContent="Center" height="100%">
                <items>
                <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
                    <f:SimpleForm id="_IDGenSimpleForm" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                                    singleContainerFullSize="true">
                        <f:content>
                            <core:Icon id="_IDGenIcon"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                height="100%"
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                    <l:GridData id="_IDGenGridData" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                            <core:Icon id="_IDGenIcon1"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                        <l:GridData id="_IDGenGridData2" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                            <core:Icon id="_IDGenIcon2"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                        <l:GridData id="_IDGenGridData3" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                        </f:content>
                    </f:SimpleForm>
                    <f:SimpleForm id="_IDGenSimpleForm1" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                                    singleContainerFullSize="true">
                        <f:content>
                            <Input id="_IDGenInput" placeholder="Enter Email">
                                <layoutData>
						            <l:GridData id="_IDGenGridData4" span="XL3 L3 M3 S12" />
					            </layoutData>
                            </Input>
                            <Input id="_IDGenInput1" type="Password" placeholder="Enter password">
                                <layoutData>
						            <l:GridData id="_IDGenGridData5" span="XL3 L3 M3 S12" />
					            </layoutData>
                            </Input>
                            <Button id="_IDGenButton" type="Accept" text="Sign In" />
                        </f:content>
                    </f:SimpleForm>
                    <f:SimpleForm id="_IDGenSimpleForm2" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
                                    singleContainerFullSize="true">
                        <f:content>
                            <Button id="_IDGenButton2" text="Create Account"/>
                			<Button id="_IDGenButton3" text="Forgot Password"/>
                        </f:content>
                    </f:SimpleForm>
                    </VBox>
                </items>
            </FlexBox>
        </VBox>
    </Page>
</mvc:View>
