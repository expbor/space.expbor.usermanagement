<mvc:View controllerName="login.login.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m"
    xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core" 
    xmlns:tnt="sap.tnt">
    <Page id="_IDGenPage" showHeader="false">
        <VBox id="_IDGenVBox2" height="100%">
        <tnt:ToolHeader id="shellLikeToolHeaderOnlyMandatoryControls" class="sapUiTinyMargin">
            <Image id="_IDGenImage" src="https://upload.wikimedia.org/wikipedia/commons/thumb/8/8f/SAP-Logo.svg/743px-SAP-Logo.svg.png" decorative="false" press="onLogoPressed" tooltip="SAP Logo">
            </Image>
            <Title id="_IDGenTitle" text="Login" wrapping="false">
            </Title>
            <ToolbarSpacer id="_IDGenToolbarSpacer" />
            <Avatar id="_IDGenAvatar" src="test-resources/sap/tnt/images/Woman_avatar_01.png" displaySize="XS" press=".onAvatarPressed" tooltip="Profile">
            </Avatar>
		</tnt:ToolHeader>
            <FlexBox id="_IDGenVBox" alignItems="Center" justifyContent="Center" height="100%">
                <items>
                <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
                    <f:SimpleForm id="registration0" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                                    singleContainerFullSize="true">
                        <f:content>
                            <core:Icon id="_IDGenIcon"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                height="100%"
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                    <l:GridData id="_IDGenGridData" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                            <core:Icon id="_IDGenIcon1"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                        <l:GridData id="_IDGenGridData2" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                            <core:Icon id="_IDGenIcon2"
                                src="sap-icon://visits"
                                color="#1661BE" 
                                class="size2"
                                press="handleStethoscopePress">
                                <core:layoutData>
                                        <l:GridData id="_IDGenGridData3" span="L4 M4 S4"/>
                                </core:layoutData>
                            </core:Icon>
                        </f:content>
                    </f:SimpleForm>
                    <f:SimpleForm id="registration" editable="true" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
                                    singleContainerFullSize="true">
                        <f:content>
                            <Input id="emailInput" placeholder="Enter Email">
                                <layoutData>
						            <l:GridData id="_IDGenGridData121" span="XL3 L3 M3 S12" />
					            </layoutData>
                            </Input>
                            <Input id="passwordInput" type="Password" placeholder="Enter password">
                                <layoutData>
						            <l:GridData id="_IDGenGridData1asd" span="XL3 L3 M3 S12" />
					            </layoutData>
                            </Input>
                            <Button id="_IDGenButton" type="Accept" text="Sign In" />
                        </f:content>
                    </f:SimpleForm>
                    <f:SimpleForm id="registration2" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12"
                                    adjustLabelSpan="true" emptySpanXL="3" emptySpanL="3" emptySpanM="3" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2"
                                    singleContainerFullSize="true">
                        <f:content>
                            <Button id="_IDGenLink0" text="Create Account"/>
                			<Button id="_IDGenLink1" text="Forgot Password"/>
                        </f:content>
                    </f:SimpleForm>
                    </VBox>
                </items>
            </FlexBox>
        </VBox>
    </Page>
</mvc:View>
